local wl = {"dawninja21", "dawninja21alt"}

local isWhitelisted = false


for _, v in pairs(wl) do
    if v == game.Players.LocalPlayer.Name then
        isWhitelisted = true
        break
    end
end


if isWhitelisted then
    print("Whitelisted!")
else
    game.Players.LocalPlayer:Kick("Not Whitelisted")
end
